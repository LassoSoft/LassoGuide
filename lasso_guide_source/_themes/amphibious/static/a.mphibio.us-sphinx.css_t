a {
	text-decoration: underline;
}

.navbar .brand {
  color: #FFF;
  text-shadow: #777 2px 2px 3px;
}

.highlight {
	margin-bottom: 1.3em;
}

{%- block sidebarlogo %}
  {%- if logo %}
.navbar h3 a, .navbar .brand {
  background: transparent url("{{ logo }}") no-repeat 22px 3px;
  padding-left: 62px;
}
  {%- endif %}
{%- endblock %}
